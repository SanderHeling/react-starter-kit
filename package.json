{
    "name": "react-starter-kit",
    "version": "1.0.0",
    "description": "React Starter Kit",
    "main": "index.js",
    "author": "Sander Heling <sander@wend.nl>",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf dist/*",
        "test": "jest",
        "test-coverage": "jest --coverage",
        "tdd": "jest --watchAll",
        "webpack:dev": "webpack-dev-server --config ./conf/webpack.config.dev.js",
        "build": "yarn run clean && webpack --config ./conf/webpack.config.prod.js",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build",
        "start": "npm run webpack:dev & npm run styleguide"
    },
    "devDependencies": {
        "autoprefixer": "7.1.4",
        "babel-core": "6.26.0",
        "babel-eslint": "8.0.0",
        "babel-jest": "21.0.2",
        "babel-loader": "7.1.2",
        "babel-preset-env": "1.6.0",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-2": "6.24.1",
        "css-loader": "0.28.7",
        "enzyme": "3.1.0",
        "enzyme-adapter-react-16": "1.0.3",
        "eslint": "4.15.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "2.9.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-prettier": "2.4.0",
        "eslint-plugin-react": "7.5.1",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.30.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "21.2.1",
        "node-sass": "4.5.3",
        "postcss-loader": "2.0.6",
        "prettier": "1.10.2",
        "react-styleguidist": "6.0.33",
        "react-test-renderer": "16.0.0",
        "sass-loader": "6.0.6",
        "stylelint": "8.1.1",
        "stylelint-config-css-modules": "1.1.0",
        "stylelint-config-standard": "17.0.0",
        "stylelint-order": "0.7.0",
        "stylelint-scss": "2.1.0",
        "url-loader": "0.5.9",
        "webpack": "3.6.0",
        "webpack-dev-server": "2.8.2",
        "webpack-merge": "4.1.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-solid": "^5.0.6",
        "@fortawesome/react-fontawesome": "^0.0.17",
        "babel-polyfill": "6.26.0",
        "classtr": "0.0.1",
        "prop-types": "15.6.0",
        "react": "16.0.0",
        "react-dom": "16.0.0"
    },
    "jest": {
        "setupFiles": [
            "./src/polyfill.js"
        ],
        "setupTestFrameworkScriptFile": "./src/__tests__/jest.setup.js",
        "testPathIgnorePatterns": [
            "./src/__tests__/jest.setup.js"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy"
        }
    }
}
