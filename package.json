{
    "name": "react-starter-kit",
    "version": "1.0.0",
    "description": "React Starter Kit",
    "main": "index.js",
    "author": "Sander Heling <sander@wend.nl>",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf dist/*",
        "test": "jest",
        "test-coverage": "jest --coverage",
        "tdd": "jest --watchAll",
        "webpack:dev": "webpack-dev-server --config ./conf/webpack.config.dev.js",
        "build": "yarn run clean && webpack --config ./conf/webpack.config.prod.js",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build",
        "start": "npm run webpack:dev & npm run styleguide"
    },
    "devDependencies": {
        "autoprefixer": "8.2.0",
        "babel-core": "6.26.0",
        "babel-eslint": "8.2.2",
        "babel-jest": "22.4.3",
        "babel-loader": "7.1.4",
        "babel-preset-env": "1.6.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-2": "6.24.1",
        "css-loader": "0.28.11",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.0.3",
        "eslint": "4.19.1",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "2.9.0",
        "eslint-loader": "2.0.0",
        "eslint-plugin-import": "2.10.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-prettier": "2.6.0",
        "eslint-plugin-react": "7.7.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "1.1.11",
        "html-webpack-plugin": "3.2.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "22.4.3",
        "node-sass": "4.8.3",
        "postcss-loader": "2.1.3",
        "prettier": "1.11.1",
        "react-styleguidist": "7.0.3",
        "react-test-renderer": "16.3.1",
        "sass-loader": "6.0.7",
        "stylelint": "9.2.0",
        "stylelint-config-css-modules": "1.2.0",
        "stylelint-config-standard": "18.2.0",
        "stylelint-order": "0.8.1",
        "stylelint-scss": "3.0.0",
        "uglifyjs-webpack-plugin": "1.2.4",
        "url-loader": "1.0.1",
        "webpack": "4.5.0",
        "webpack-cli": "2.0.14",
        "webpack-dev-server": "3.1.1",
        "webpack-merge": "4.1.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "1.1.5",
        "@fortawesome/fontawesome-free-solid": "5.0.9",
        "@fortawesome/react-fontawesome": "0.0.18",
        "babel-polyfill": "6.26.0",
        "classtr": "0.0.1",
        "prop-types": "15.6.1",
        "react": "16.3.1",
        "react-dom": "16.3.1",
        "whatwg-fetch": "2.0.4"
    },
    "jest": {
        "setupFiles": [
            "./src/polyfill.js"
        ],
        "setupTestFrameworkScriptFile": "./src/__tests__/jest.setup.js",
        "testPathIgnorePatterns": [
            "./src/__tests__/jest.setup.js"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy"
        }
    }
}
